{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "off",
    "no-undef": "off",
    "array-element-newline": ["error", {
      "ArrayExpression": "consistent",
      "ArrayPattern": { "minItems": 3 }
    }]
  }
}
